    "import socket\n",
    "\n",
    "def sendToPetBuddy(msg):\n",
    "    sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)\n",
    "    sock.connect(('localhost',6468))\n",
    "    sock.sendall(msg.encode()) \n",
    "\n",
    "    \n",
    "#function used in message = str    \n",
    "message = \"ping\"\n",
    "sendToPetBuddy(message)"
